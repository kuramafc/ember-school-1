<h1>Chef Tracker</h1>
<ul>
  {{#each chefs as |chef|}}
    {{#if chef.avability}}
      <li class="avalable" >{{chef.name}}
        <button {{action 'exit' chef}}>Exit</button>
      </li>
      {{else}}
        <li class="not-avalable" >{{chef.name}}
          <button {{action 'enter' chef}}>Enter</button>
        </li>
    {{/if}}
  {{/each}}
</ul>